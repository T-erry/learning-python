class Person():

    def __init__(self, name, id):
        self.name = name
        self.id = id


    def Display(self):
        print(self.name , self.id)




emp = Person("Terry", 101)

emp.Display()

# Creating a Child Class
class Emp(Person):
    def print(self):
        print("Emp class is called")


Emp_details = Emp("Mark", 102)
# calling parent class function
Emp_details.Display()
# Calling child class function
Emp_details.print()



